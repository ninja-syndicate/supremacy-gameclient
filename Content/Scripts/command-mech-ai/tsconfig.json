{
     "compilerOptions": {
          "target": "ES6",
          "module": "commonjs",
          "moduleResolution": "node",
          "lib": ["es5", "es6", "dom"],
          "baseUrl": "src",
          "rootDir": "src",
          "paths": {
               "@root/*": ["*"],
               "@tasks/*": ["tasks/*"],
               "@trees/*": ["trees/*"],
               "@blackboards/*": ["blackboards/*"],
               "@decorators/*": ["decorators/*"],
               "@predicates/*": ["predicates/*"],
               "@branches/*": ["branches/*"],
               "*": ["types/*"]
          }
     },
}